# common Makefile
#


all: tcanetpp tcaxml++ tcasnmp tcasql++ tnmsCore


.PHONY: tcanetpp
tcanetpp:
	( cd tcanetpp; make all )
	@echo

.PHONY: tcaxml++
	( cd tcaxml++; make all )
	@echo

.PHONY: tcasnmp
tcasnmp:
	( cd tcasnmp; make all )
	@echo

.PHONY: tcasql++
tcasql++:
	( cd tcasql++; make all )
	@echo

.PHONY: tnmsCore
tnmsCore:
	( cd tnmsCore; make all )
	@echo



